   "source": [
    "def inspect(func):\n",
    "  def wraped_func(*args, **kwargs):\n",
    "    print(f\"Function name: {func.__name__}\")\n",
    "    val = func(*args, **kwargs)\n",
    "    print(val)\n",
    "    return val\n",
    "  return wraped_func\n",
    "\n",
    "@inspect\n",
    "def combine(a, b):\n",
    "  return a + b\n",
    "\n",
    "print(\"Hasilnya adalah: \" + str(combine(1,3)))"
   ]
  }
